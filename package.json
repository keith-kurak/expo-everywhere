{
  "name": "expo-everywhere",
  "version": "1.0.0",
  "homepage": "http://llamaluvr.github.io/expo-everywhere",
  "main": "dist/index.js",
  "scripts": {
    "start": "expo start",
    "electron-start": "yarn expo-electron start",
    "electron-build": "yarn electron-webpack && yarn electron-builder --dir -c.compression=store -c.mac.identity=null",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "postinstall": "patch-package",
    "web-build": "expo build:web",
    "web-deploy": "gh-pages -d web-build",
    "build-package": "npx babel app --out-dir dist --ignore \"**/*.test.js\""
  },
  "dependencies": {
    "@expo/vector-icons": "^10.2.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/bottom-tabs": "^5.7.2",
    "@react-navigation/native": "^5.7.1",
    "@react-navigation/stack": "^5.7.1",
    "@react-navigation/tabs": "^0.0.0-alpha.12",
    "axios": "^0.19.2",
    "axios-mock-adapter": "^1.18.2",
    "expo": "~38.0.8",
    "expo-status-bar": "^1.0.2",
    "lodash": "^4.17.19",
    "lorem-ipsum-react-native": "^1.0.3",
    "luxon": "^1.24.1",
    "moment": "^2.27.0",
    "patch-package": "^6.2.2",
    "postinstall-postinstall": "^2.1.0",
    "react": "~16.11.0",
    "react-dom": "~16.11.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-38.0.2.tar.gz",
    "react-native-gesture-handler": "~1.6.0",
    "react-native-gifted-chat": "^0.16.3",
    "react-native-iphone-x-helper": "^1.2.1",
    "react-native-material-initials": "^0.0.12",
    "react-native-reanimated": "~1.9.0",
    "react-native-safe-area-context": "~3.0.7",
    "react-native-screens": "~2.9.0",
    "react-native-web": "~0.11.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.8.6",
    "@expo/electron-adapter": "^0.0.7",
    "@expo/webpack-config": "^0.12.20",
    "babel-preset-expo": "~8.1.0",
    "electron": "^6.0.12",
    "electron-builder": "^22.7.0",
    "gh-pages": "^3.1.0"
  },
  "private": true
}
